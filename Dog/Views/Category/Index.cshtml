@model IEnumerable<CatViewModel>

<h1>Categories</h1>

<table>
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>
    </tr>
    @foreach (var category in Model)
    {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
            </tr>
    }
</table>

