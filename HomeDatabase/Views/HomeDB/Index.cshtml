


<div class="text-center align-text-top">
    <h3>Tables of Database: HomeDB</h3>
</div>


<table class="table">
    <tbody>
        @foreach (var table in Model)
        {
            <tr>
                <td>@table.TableName</td>
                <td>
                    <a class="btn btn-secondary" asp-action="Index" asp-route-id="@table.TableName" asp-controller="Servers">Select</a>
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="@table.TableName" asp-controller="HomeDB" >Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


<div>
    <a class="btn btn-primary" asp-action="Create" asp-controller="HomeDB">Create new Table</a>
    <a class="btn btn-secondary" href="@Url.Action("GoBack", "HomeDB")">Back</a>
    <input asp-controller="File" asp-action="Download" type="submit" value="Download" class="btn btn-primary" />
</div>


