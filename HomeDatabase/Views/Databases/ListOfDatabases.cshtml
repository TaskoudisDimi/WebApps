





<div class="text-center align-text-top">
    <h3>Databases</h3>
</div>

<div class="text-center mt-5">
    <ul>
        @foreach (var databases in Model)
        {
            if (databases.Name != null)
            {
                <li class="mt-2" style="list-style-type: none;">
                    <a class="btn btn-primary" asp-area="" asp-action="database" asp-controller="Databases"
               asp-route-Name="@databases.Name">@databases.Name</a>
                </li>
                
            }

        }
    </ul>
</div>

<div>
    <a class="btn btn-primary" asp-area="" asp-action="Create" asp-controller="Databases">Create new Database</a>
    <a class="btn btn-danger" asp-area="" asp-action="Delete" asp-controller="Databases">Delete Database</a>
    <a class="btn btn-secondary" href="@Url.Action("GoBack", "Databases")">Back</a>
    <input asp-controller="File" asp-action="Download" type="submit" value="Download" class="btn btn-primary" />
</div>


